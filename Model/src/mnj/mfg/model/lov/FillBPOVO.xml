    <![CDATA[/* Formatted on 3/9/2020 6:42:37 PM (QP5 v5.256.13226.35510) */
-----Query Updated By Syed Abdul Ahad on 7/11/2019-----
-----Query Updated By Mr. Sakibul Islam on 09.Mar.2020; Added Buyer, Season Info-----
SELECT ll.buyer_id,
       ll.attribute1 buyer_name,
       ll.season,
       l.attribute1,
       NVL ( (SELECT OOHA.ATTRIBUTE12
                FROM OE_ORDER_HEADERS_ALL OOHA
               WHERE OOHA.CUST_PO_NUMBER = l.attribute1 AND ROWNUM = 1),
            l.attribute1)
          CURRENT_BPO,
       l.attribute2,
       l.attribute3,
       TO_NUMBER (l.attribute4) quantity,
       ll.style_name,
       h.transaction_type
  FROM MNJ_MFG_ISSUE_REC_WASH_H h,
       MNJ_MFG_ISSUE_REC_WASH_L_H ll,
       MNJ_MFG_ISSUE_REC_WASH_D l
 WHERE     h.header_id = ll.header_id
       AND ll.line_id = l.header_id
       AND h.transaction_type = 'Ready For Finish Department'
--AND H.HEADER_ID=27816
---------From here added by Syed Abdul Ahad on 7/11/2019-------
UNION ALL
SELECT L.buyer_id,
       L.attribute1 buyer_name,
       L.season,
       D.ATTRIBUTE1,
       NVL ( (SELECT OOHA.ATTRIBUTE12
                FROM OE_ORDER_HEADERS_ALL OOHA
               WHERE OOHA.CUST_PO_NUMBER = L.ATTRIBUTE1 AND ROWNUM = 1),
            D.ATTRIBUTE1)
          CURRENT_BPO,
       D.ATTRIBUTE2,
       D.ATTRIBUTE3,
       D.QUANTITY,
       L.STYLE_NAME,
       H.TRANSACTION_TYPE
  --,H.RECEIVE_NO,H.TRANSACTION,H.WASH_EMB_PRINT_INVORG,D.PRODUCTION_TYPE,D.WASH_TYPE,D.ISSUANCE_CHALLAN_NO
  FROM MNJ_MFG_ISSUE_REC_WASH_H H,
       MNJ_MFG_ISSUE_REC_WASH_L_H L,
       MNJ_MFG_ISSUE_REC_WASH_L D
 WHERE     1 = 1
       AND H.HEADER_ID = L.HEADER_ID
       AND D.HEADER_ID = L.LINE_ID                     --AND H.HEADER_ID=27816
       AND H.TRANSACTION_TYPE = 'Ready For Finish Department']]>
  </SQLQuery>
